<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<!-- <!DOCTYPE hibernate-mapping SYSTEM "../dtd/hibernate-mapping-2.0.dtd"> -->

<hibernate-mapping package="br.edu.cba.ifmt.Model">

    <class name="br.edu.cba.ifmt.Model.City" table="municipios">
        <id name="id" column="id">
            <generator class="sequence">
            	<param name="sequence">municipios_id_seq</param>
            </generator>
        </id>

        <property name="nome" column="nome" type="string" not-null="true"/>
    </class>

</hibernate-mapping>